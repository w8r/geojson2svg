{
  "name": "geojson-to-svg",
  "version": "1.1.4",
  "description": "primitive SVG renderer for geojson with feature styles",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "geojson2svg": "bin/geojson2svg",
    "measure-font": "bin/measure_font"
  },
  "directories": {
    "test": "test",
    "bin": "bin",
    "src": "src",
    "fonts": "fonts"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "demo": "node test/render.js",
    "lint": "eslint ./src/",
    "build-js": "browserify index.js -v -o dist/geojson-to-svg-src.js",
    "compress-js": "uglifyjs dist/geojson-to-svg-src.js -o dist/geojson-to-svg.js -m --comments",
    "build": "vite build && npm run types",
    "types": "tsc -d --emitDeclarationOnly --declarationDir dist/types src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w8r/geojson2svg.git"
  },
  "keywords": [
    "geojson",
    "renderer",
    "svg",
    "stylesheet",
    "gis"
  ],
  "author": "Alexander Milevski <info@w8r.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w8r/geojson2svg/issues"
  },
  "homepage": "https://github.com/w8r/geojson2svg#readme",
  "devDependencies": {
    "@types/string-hash": "^1.1.1",
    "browserify": "^17.0.0",
    "deep-extend": "^0.6.0",
    "eslint": "^6.8.0",
    "lodash.flatten": "^4.4.0",
    "nodemon": "^2.0.6",
    "simplify-js": "^1.2.1",
    "svg-polygon-decorator": "^1.0.7",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "typescript": "^4.9.4",
    "uglifyjs": "^2.4.10",
    "vite": "^4.0.4",
    "vitest": "^0.27.1"
  },
  "dependencies": {
    "@sindresorhus/string-hash": "^2.0.0",
    "@types/geojson": "^7946.0.10",
    "geojson-project": "^2.0.1",
    "gl-matrix": "^3.4.3",
    "json-extend": "^0.1.0",
    "minimist": "^1.2.5",
    "string-hash": "^1.1.0",
    "transformation-matrix-js": "^2.6.5"
  },
  "optionalDependencies": {
    "nightmare": "^3.0.2"
  }
}
