{
  "name": "geojson2svg",
  "version": "1.0.0",
  "description": "primitive SVG renderer for geojson with feature styles",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/*.test.js",
    "test:watch": "nodemon --exec 'npm test'",
    "build-js": "browserify index.js -v -o dist/geojson2svg-src.js",
    "compress-js": "uglifyjs dist/geojson2svg-src.js -o dist/geojson2svg.js -m --comments",
    "build": "npm run build-js && npm run compress-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w8r/geojson2svg.git"
  },
  "keywords": [
    "geojson",
    "renderer",
    "svg",
    "stylesheet",
    "gis"
  ],
  "author": "Alexander Milevski <info@w8r.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w8r/geojson2svg/issues"
  },
  "homepage": "https://github.com/w8r/geojson2svg#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "deep-extend": "^0.4.1",
    "eslint": "^3.4.0",
    "html-validator": "^2.0.1",
    "nightmare": "^2.7.0",
    "nodemon": "^1.10.2",
    "svg-polygon-decorator": "^1.0.7",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "geojson-project": "^1.0.0",
    "json-extend": "^0.1.0",
    "minimist": "^1.2.0",
    "string-hash": "^1.1.0",
    "transformation-matrix-js": "^2.6.5"
  }
}
